<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>create or replace procedure drop_tbl_table(tname in varchar)

is
    v_sql      varchar(100);    -- åŠ¨æ€SQLè¯­å¥
    v_count    number(10);      -- æ•°æ®åº“ä¸­dropè¿‡çš„è¡¨æ•°


begin
    v_count := 0;
    
    for v1 in (select * from all_tables where owner = 'MMSG')
    loop
        v_sql := 'drop table ' || v1.table_name;
        v_count := v_count + 1;
        execute immediate v_sql;
    end loop;
    dbms_output.put_line(v_count);
end drop_tbl_table;
/
</code></pre></div></div>

